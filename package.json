{
  "name": "@remsfal/remsfal-frontend",
  "version": "0.0.8-SNAPSHOT",
  "type": "module",
  "homepage": "https://remsfal.de",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remsfal/remsfal-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/remsfal/remsfal-frontend/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-s type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "preview": "vite preview",
    "test": "run-s test:unit test:e2e",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "cypress": "cypress open",
    "cypress:dev": "run-s test:e2e:dev",
    "coverage": "run-s coverage:clean coverage:unit coverage:e2e coverage:merge",
    "coverage:clean": "rm -rf coverage coverage-vitest coverage-cypress .nyc_output",
    "coverage:unit": "vitest run --coverage",
    "coverage:e2e": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress run --e2e'",
    "coverage:merge": "node scripts/merge-coverage.js",
    "lint": "eslint --quiet",
    "lint:report": "mkdir -p reports && eslint --format json --output-file reports/eslint-report.json",
    "lint:fix": "eslint --fix",
    "stylelint": "stylelint '{src,public}/**/*.{vue,css,scss}'",
    "stylelint:fix": "stylelint --fix '{src,public}/**/*.{vue,css,scss}'",
    "openapi": "run-p openapi:ticketing openapi:platform openapi:notification",
    "openapi:ticketing": "openapi-typescript openapi/ticketing-spec.json -o src/services/api/ticketing-schema.ts",
    "openapi:platform": "openapi-typescript openapi/platform-spec.json -o src/services/api/platform-schema.ts",
    "openapi:notification": "openapi-typescript openapi/notification-spec.json -o src/services/api/notification-schema.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@primevue/themes": "^4.5.4",
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "idb": "^8.0.3",
    "lightningcss": "^1.30.2",
    "msw": "^2.12.3",
    "pinia": "^3.0.3",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "vue": "^3.5.21",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^11.2.2",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.7",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@pinia/testing": "^1.0.2",
    "@primevue/auto-import-resolver": "^4.4.1",
    "@redocly/cli": "^2.11.1",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tsconfig/node-lts": "^24.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.8.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-istanbul": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.9.4",
    "chai": "^6.2.0",
    "cypress": "^15.5.0",
    "eslint": "^9.38.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^10.5.1",
    "flush-promises": "^1.0.2",
    "globals": "^16.4.0",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^8.0.4",
    "nyc": "^17.1.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
    "sass": "^1.96.0",
    "sinon": "^21.0.0",
    "start-server-and-test": "^2.1.2",
    "stylelint": "^16.25.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^4.1.14",
    "tailwindcss-primeui": "^0.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.2.7",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^4.0.3",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "glob": "^9.3.2",
    "vite-plugin-vue-devtools": {
      "vite-plugin-inspect": "^11.3.0"
    }
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.53.3"
  }
}
